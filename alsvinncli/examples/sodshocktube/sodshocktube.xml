<config>
<fvm>
  <name>
    sodtube
  </name>
  <platform>cpu</platform>
  <grid>
    <lowerCorner>-5 0 0</lowerCorner>
    <upperCorner>5 0 0</upperCorner>
    <dimension>4096 1 1</dimension>
  </grid>
  <boundary>neumann</boundary>
  <flux>hll3</flux>
  <endTime>1.3</endTime>
  <equation>euler1</equation>
  <reconstruction>none</reconstruction>
  <cfl>0.45</cfl>
  <integrator>rungekutta2</integrator>
  <equationParameters>
      <gamma>1.4</gamma>
  </equationParameters>
  <initialData>
    <python>sodshocktube.py</python>
    <parameters>
      <parameter>
        <name>X</name>
        <length>1</length>
        <value>0</value>
      </parameter>
    </parameters>
  </initialData>
    <diffusion>
        <name>none</name>
        <reconstruction>none</reconstruction>
    </diffusion>
  <writer>
    <type>netcdf</type>
    <basename>sodshocktube</basename>
    <numberOfSaves>10</numberOfSaves>
  </writer>
</fvm>
<uq>
  <samples>1024</samples>
  <generator>auto</generator>
  <parameters>
    <parameter>
      <name>X</name>
      <length>1</length>
      <type>uniform</type>
    </parameter>
  </parameters>
</config>
